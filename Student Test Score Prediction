    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Running Fixed GPU Training...\n",
      " Fold 1 Complete\n",
      " Fold 2 Complete\n",
      " Fold 3 Complete\n",
      " Fold 4 Complete\n",
      " Fold 5 Complete\n",
      "Done! submission.csv is ready for download.\n"
     ]
    }
   ],
   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "from catboost import CatBoostRegressor\n",
    "from sklearn.model_selection import KFold\n",
    "from sklearn.metrics import mean_squared_error\n",
    "\n",
    "# 1. LOAD DATA\n",
    "train = pd.read_csv(\"/kaggle/input/playground-series-s6e1/train.csv\")\n",
    "test = pd.read_csv(\"/kaggle/input/playground-series-s6e1/test.csv\")\n",
    "\n",
    "# 2. ESSENTIAL FEATURE ENGINEERING (Clean & Numeric)\n",
    "def engineering(df):\n",
    "    df.columns = df.columns.str.strip().str.lower()\n",
    "    \n",
    "    # Map Ordinal Categories to numbers so they don't cause float errors\n",
    "    quality_map = {'poor': 1, 'average': 2, 'good': 3}\n",
    "    difficulty_map = {'easy': 1, 'moderate': 2, 'hard': 3}\n",
    "    \n",
    "    # Directly replace the text with numbers\n",
    "    df['sleep_quality'] = df['sleep_quality'].str.lower().map(quality_map).fillna(2)\n",
    "    df['exam_difficulty'] = df['exam_difficulty'].str.lower().map(difficulty_map).fillna(2)\n",
    "    df['facility_rating'] = df['facility_rating'].str.lower().map({'low': 1, 'medium': 2, 'high': 3}).fillna(2)\n",
    "    \n",
    "    # Interactions\n",
    "    df['productivity'] = df['study_hours'] * (df['class_attendance'] / 100)\n",
    "    df['rest_impact'] = df['sleep_hours'] * df['sleep_quality']\n",
    "    \n",
    "    return df\n",
    "\n",
    "train = engineering(train)\n",
    "test = engineering(test)\n",
    "\n",
    "target = 'exam_score'\n",
    "# These are the ONLY columns that remain as text (strings)\n",
    "cat_features = ['gender', 'course', 'internet_access', 'study_method']\n",
    "\n",
    "# 3. TARGET ENCODING\n",
    "for col in cat_features:\n",
    "    mapping = train.groupby(col)[target].mean()\n",
    "    train[f'{col}_te'] = train[col].map(mapping)\n",
    "    test[f'{col}_te'] = test[col].map(mapping)\n",
    "\n",
    "features = [col for col in train.columns if col not in ['id', target]]\n",
    "X, y = train[features], train[target]\n",
    "X_test = test[features]\n",
    "\n",
    "# 4. FAST 5-FOLD CV ON GPU\n",
    "n_splits = 5 \n",
    "kf = KFold(n_splits=n_splits, shuffle=True, random_state=42)\n",
    "test_preds = np.zeros(len(test))\n",
    "\n",
    "print(\"Running Fixed GPU Training...\")\n",
    "\n",
    "for fold, (train_idx, val_idx) in enumerate(kf.split(X, y)):\n",
    "    X_train, X_val = X.iloc[train_idx], X.iloc[val_idx]\n",
    "    y_train, y_val = y.iloc[train_idx], y.iloc[val_idx]\n",
    "    \n",
    "    model = CatBoostRegressor(\n",
    "        iterations=2000,\n",
    "        learning_rate=0.05,\n",
    "        task_type=\"GPU\",       # Uses your GPU T4 x2\n",
    "        devices='0',\n",
    "        depth=6,\n",
    "        loss_function='RMSE',\n",
    "        verbose=0,\n",
    "        cat_features=cat_features, # Explicitly handles the remaining text columns\n",
    "        early_stopping_rounds=100\n",
    "    )\n",
    "    \n",
    "    model.fit(X_train, y_train, eval_set=(X_val, y_val), use_best_model=True)\n",
    "    test_preds += model.predict(X_test) / n_splits\n",
    "    print(f\" Fold {fold+1} Complete\")\n",
    "\n",
    "# 5. SUBMISSION\n",
    "submission = pd.DataFrame({\"id\": test[\"id\"], \"exam_score\": test_preds})\n",
    "submission.to_csv(\"submission.csv\", index=False)\n",
    "print(\"Done! submission.csv is ready for download.\")"
   ]
  }
